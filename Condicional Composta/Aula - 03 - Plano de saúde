#include <stdio.h>
#include <string.h>

int main() {
  char nome[30];
  int idade;

  printf("\nInforme seu nome: ");
  scanf("%s", nome);
  printf("\nInforme sua idade: ");
  scanf("%d", &idade);


      if (idade<18)
      {
        printf("\n%s,  valor da mensalidade do plano de saúde é de R$50,00",nome);
      }
      else if (idade>19 && idade<29)
      {
        printf("\n%s, o valor da mensalidade do plano de saúde é de R$70,00", nome);
      }
      else if (idade>30 && idade<45)
      {
        printf("\n%s, o valor da mensalidade do plano de saúde é de R$90,00", nome);
      }
      else if (idade>46 && idade<65)
      {
        printf("\n%s, o valor da mensalidade do plano de saúde é de R$130,00", nome);
      }
      else (idade>65)
        printf("\n%s, o valor da mensalidade do plano de saúde é de R$170,00", nome);

  return 0;
}
