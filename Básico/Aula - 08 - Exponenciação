#include <stdio.h>
#include <math.h>

int main(void) {

  float num1, num2, total;

  printf("Informe o primeiro número:");
  scanf("%f", &num1);
  printf("Informe o segundo número:");
  scanf("%f", &num2);

  total= pow(num1, num2);

  printf("\%.2f elevado a %.2f é %.2f", num1, num2, total);


  return 0;
}
